" font size
set guifont=Fira\ Mono:h14
"set guifont=monoOne:h14
"set guifont=CamingoCode:h13
" window size
set lines=50 columns=90

" line highlight
set cursorline
" disable scrollbar
set guioptions -=L
set guioptions -=l
set guioptions -=R
set guioptions -=r
" Disable all blinking:
:set guicursor+=a:blinkon0
