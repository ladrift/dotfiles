# This is Git's per-user configuration file.
[core]
# Please adapt and uncomment the following lines:
#	user = ladrift
#	email = cxfyzy@gmail.com
[core]
    editor = vim
    autocrlf = input
[user]
    name = "Ziyi Yan"
    email = cxfyzy@gmail.com
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
